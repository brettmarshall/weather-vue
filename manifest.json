{
	"name": "WeatherVue",
	"description": "5 Day Forcast",
	"version": "1.0.0",
	"manifest_version": 2,
	"icons": {
		"128": "dist/images/sunny.png"
	},
	"chrome_url_overrides" : { 
		"newtab": "index.html"
	},
	"permissions": [
		"http://localhost/",
		"http://api.aerisapi.com/",
		"http://geoiplookup.wikimedia.org/"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}